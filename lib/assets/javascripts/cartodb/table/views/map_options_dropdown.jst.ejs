<% var bbgProUI = $.inArray("bbg_pro_ui", user_data.feature_flags) !== -1; %>
<% var profAttrs = new cdb.admin.ProfileAttributes(profile_attributes); %>
<% console.log(profAttrs); %>

<div class="title"><h2>Display Options</h2></div>
<ul class="options switches">
  <li class="title"><p>Fixed title</p></li>
  <li class="description"><p>Fixed description</p></li>
  <li class="search"><p>Location Search</p></li>
  <% if (bbgProUI) { %>
    <li class="zoom"><p>Zoom controls</p></li>
    <li class="scrollwheel"><p>Scroll to Zoom</p></li>
    <li class="layer_selector"><p>Layer selector</p></li>
  <% } %>
  <li class="legends"><p>Legends</p></li>
  <% if (bbgProUI) { %>
    <li class="fullscreen"><p>Fullscreen</p></li>
    <li class="inset_map"><p>Inset map</p></li>
    <li class="logo"><p>CartoDB Logo</p></li>
  <% } %>
  <% if (profAttrs.hasAttribute("enableLockPan") && profAttrs.getAttribute("enableLockPan")) { %>
    <li class="lock_pan"><p>Lock Pan</p></li>
  <% } %>
  <% if (profAttrs.hasAttribute("enableLockZoom") && profAttrs.getAttribute("enableLockZoom")) { %>
    <li class="lock_zoom"><p>Lock Zoom</p></li>
  <% } %>
</ul>
